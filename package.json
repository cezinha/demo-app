{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "react-devtool": "react-devtools",
    "redux-devtool": "remotedev",
    "lint": "eslint . && stylelint '**/*.@(css|scss|sass|less|styl)'"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/netinfo": "~2.0.10",
    "expo": "^33.0.0",
    "expo-asset": "^6.0.0",
    "expo-background-fetch": "~5.0.1",
    "expo-camera": "~5.0.1",
    "expo-font": "~5.0.1",
    "expo-permissions": "~5.0.1",
    "expo-sqlite": "~5.0.1",
    "expo-task-manager": "~5.0.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "pusher": "^3.0.0",
    "pusher-js": "^5.0.3",
    "react": "16.8.3",
    "react-dom": "^16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-web": "^0.11.4",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-preset-expo": "^5.1.1",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-react-native": "^3.7.0",
    "fetch-mock": "^7.3.9",
    "jest": "^24.8.0",
    "jest-css-modules-transform": "^2.5.0",
    "jest-expo": "^33.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-devtools": "^3.6.3",
    "react-native-sass-transformer": "^1.4.0",
    "reactotron-react-native": "^3.6.4",
    "reactotron-redux": "^3.1.1",
    "reactotron-redux-saga": "^4.2.2",
    "redux-mock-store": "^1.5.3",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1",
    "stylelint": "^10.1.0",
    "stylelint-config-react-native-css-modules": "^3.0.0",
    "stylelint-react-native": "^2.1.1"
  },
  "private": true,
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  },
  "jest": {
    "verbose": true,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    }
  }
}
